
<div class="row justify-content-md-center">
	<div class="col-md-6">
		<h3>Select a user: </h3>
		<ul class="people_list">
			{{#each user}}
	    	<li><a class="btn btn-dark btn-block" data-attribute={{id}} role="button">{{username}}</a></li>
	  		{{/each}}
		</ul>
	</div>
</div>



